{
  "name": "eslint-plugin-botland",
  "version": "3.1.0",
  "description": "Lint your botland JS scripts",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js package.json rules/ test/ configs/ --ext js,json",
    "test:js": "nyc ava",
    "test": "npm run lint && npm run test:js",
    "coverage": "nyc report && codecov",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freaktechnik/eslint-plugin-botland.git"
  },
  "author": "Martin Giger (https://humanoids.be)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freaktechnik/eslint-plugin-botland/issues"
  },
  "homepage": "https://github.com/freaktechnik/eslint-plugin-botland#readme",
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@freaktechnik/eslint-config-node": "^7.1.0",
    "@freaktechnik/eslint-config-test": "^7.1.0",
    "ava": "^3.8.2",
    "codecov": "^3.7.0",
    "eslint": "^6.8.0",
    "eslint-ava-rule-tester": "^4.0.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "nyc": "^15.1.0"
  },
  "peerDependencies": {
    "eslint": ">=5.4.0"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "botland"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "index.js",
    "README.md",
    "LICENSE",
    "api.json",
    "rules/*",
    "configs/*"
  ],
  "ava": {
    "babel": true
  },
  "snyk": true,
  "dependencies": {
    "snyk": "^1.319.2"
  }
}
