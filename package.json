{
  "name": "eslint-plugin-botland",
  "version": "3.0.0",
  "description": "Lint your botland JS scripts",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js package.json rules/ test/ configs/ --ext js,json",
    "test:js": "nyc ava",
    "test": "npm run lint && npm run test:js",
    "coverage": "nyc report && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freaktechnik/eslint-plugin-botland.git"
  },
  "author": "Martin Giger (https://humanoids.be)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freaktechnik/eslint-plugin-botland/issues"
  },
  "homepage": "https://github.com/freaktechnik/eslint-plugin-botland#readme",
  "devDependencies": {
    "@freaktechnik/eslint-config-node": "^6.6.2",
    "@freaktechnik/eslint-config-test": "^6.6.2",
    "ava": "^1.4.1",
    "codecov": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-ava-rule-tester": "^3.0.0",
    "eslint-plugin-eslint-plugin": "^2.0.1",
    "nyc": "^14.0.0"
  },
  "peerDependencies": {
    "eslint": ">=5.4.0"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "botland"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "index.js",
    "README.md",
    "LICENSE",
    "api.json",
    "rules/*",
    "configs/*"
  ]
}
